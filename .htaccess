RewriteEngine On
RewriteCond %{HTTP_HOST} myfuhsicbt\.online [NC]
RewriteCond %{SERVER_PORT} 80
RewriteRule ^(.*)$ https://myfuhsicbt.online/$1 [R,L]

RewriteEngine on 
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.php -f
RewriteRule ^(.*)$ $1.php
RewriteCond %{REQUEST_FILENAME}\.html -f
RewriteRule ^(.*)$ $1.html
